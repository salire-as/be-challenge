{
  "name": "user",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/user/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/user",
        "main": "apps/user/src/main.ts",
        "tsConfig": "apps/user/tsconfig.app.json",
        "assets": ["apps/user/src/assets"],
        "webpackConfig": "apps/user/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "user:build",
        "inspect": false
      },
      "configurations": {
        "production": {
          "buildTarget": "user:build:production"
        }
      }
    },
    "debug": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "user:build",
        "inspect": false,
        "runtimeArgs": ["--inspect=9090"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/user/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/user/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
